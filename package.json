{
  "name": "learn-next",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "npm run generate && npm run build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus && npm -s run generate:client",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api",
    "generate:client": "graphql-codegen",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.4.2",
    "apollo-server-micro": "^3.5.0",
    "graphql": "^15.7.2",
    "graphql-scalars": "^1.13.1",
    "graphql-tag": "^2.12.6",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.0.3",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/typescript": "^2.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.0",
